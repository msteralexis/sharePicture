create table user (id int not null primary key auto_increment, nom varchar(50), prenom varchar(50), mail varchar(50), mdp varchar(50) );

create table album (id int not null primary key auto_increment, nom varchar(50), affiche int, urlalbum varchar(50), iduser int );

create table albumphoto( album int, photo int );


create table photo (id int not null primary key auto_increment, nom varchar(50), image blob, idalbum int );


